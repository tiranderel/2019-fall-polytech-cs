def setup():
    size (500, 500)
    smooth()
    noLoop()
    
def draw():
    background(50)
    
    noStroke()
    fill(94, 206, 40, 50)
    strokeWeight(110)
    ellipse(250, 100, 200, 200)
    
    noStroke()
    fill(94, 206, 40, 100)
    ellipse(250, 200, 200, 200)
    
    noStroke()
    fill(94, 206, 40, 150)
    ellipse(250, 300, 200, 200)
    
    noStroke()
    fill(94, 206, 40, 150)
    ellipse(250, 400, 200, 200)
    
    
    
    
    
    
